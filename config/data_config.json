{   "DATA_GRAPHS_LOCATION": "/data/",
    "CONTEXT_GRAPHS_LOCATION" : "/units/",
    "MAX_BATCH_SIZE" : 1000,
    "START_SEARCH_INDEX": 95000,
    "DEFAULT_DATE": "2022-04-12 00:00:00",
    "data_global_uris":{
    "TO_IGNORE":[
        "https://biomedit.ch/rdf/sphn-ontology/sphn#hasCodeCodingSystemAndVersion",
        "https://biomedit.ch/rdf/sphn-ontology/sphn#hasCodeName",
        "https://biomedit.ch/rdf/sphn-ontology/sphn#hasIdentifier",
        "http://www.w3.org/1999/02/22-rdf-syntax-ns#type"
    ],
    "ENTRY_DATA_CONCEPTS": [
        "https://biomedit.ch/rdf/sphn-ontology/sphn#FOPHDiagnosis",
        "https://biomedit.ch/rdf/sphn-ontology/sphn#LabResult",
        "https://biomedit.ch/rdf/sphn-ontology/sphn#AdministrativeGender",
        "https://biomedit.ch/rdf/sphn-ontology/sphn#Biosample"
    ]
},
    "COLUMNS_MAPPING": {
        "VALUE":{
        "http://www.w3.org/2001/XMLSchema#integer" : {"col": "NVAL_NUM", "misc":{"VALTYPE_CD":"N", "TVAL_CHAR":"E"}},
        "http://www.w3.org/2001/XMLSchema#double" : {"col": "NVAL_NUM","misc":{"VALTYPE_CD":"N", "TVAL_CHAR":"E"}},
        "http://www.w3.org/2001/XMLSchema#float" : {"col": "NVAL_NUM","misc":{"VALTYPE_CD":"N", "TVAL_CHAR":"E"}},
        "http://www.w3.org/2001/XMLSchema#string" : {"col": "TVAL_CHAR","misc":{"VALTYPE_CD":"T"}}
        },
        "CONTEXT":{
        "https://biomedit.ch/rdf/sphn-ontology/sphn#SubjectPseudoIdentifier"  : {
            "col": "PATIENT_NUM",  
            "overwrite": "False",
            "pred_to_value": ["https://biomedit.ch/rdf/sphn-ontology/sphn#hasIdentifier"],
            "table" : [["PATIENT_DIMENSION", "PATIENT_NUM"]]
        },
        "https://biomedit.ch/rdf/sphn-ontology/sphn#AdministrativeCase"  : {
            "col": "ENCOUNTER_NUM", 
            "overwrite": "False",
            "pred_to_value": ["https://biomedit.ch/rdf/sphn-ontology/sphn#hasIdentifier"],
            "table" : [["VISIT_DIMENSION", "ENCOUNTER_NUM"], ["VISIT_DIMENSION", "PATIENT_NUM"]]
        },
        "https://biomedit.ch/rdf/sphn-ontology/sphn#DataProviderInstitute"    : {
            "col": "PROVIDER_ID",  
            "overwrite": "False",
            "verbose_value":["https://biomedit.ch/rdf/sphn-ontology/sphn#hasDataProviderInstituteCode", "https://biomedit.ch/rdf/sphn-ontology/sphn#hasCodeName"],
            "pred_to_value":["https://biomedit.ch/rdf/sphn-ontology/sphn#hasDataProviderInstituteCode", "https://biomedit.ch/rdf/sphn-ontology/sphn#hasIdentifier"],
            "table" : [["PROVIDER_DIMENSION", "PROVIDER_ID"]]
        },
        "http://www.w3.org/2001/XMLSchema#dateTime"   : {
            "col": "START_DATE",
            "overwrite": "False"},
        "https://biomedit.ch/rdf/sphn-ontology/sphn#Unit"  : {
            "col": "UNITS_CD", 
            "overwrite": "True",
            "pred_to_value": ["https://biomedit.ch/rdf/sphn-ontology/sphn#hasUnitCode", "http://www.w3.org/2000/01/rdf-schema#label"]}
    }
    }
    
}
